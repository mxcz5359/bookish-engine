# 规则匹配
# 此规则部分没有做防泄露处理，因为弊严重大于利！
rules:
  - DOMAIN-SUFFIX,qichiyu.com,🚀 节点选择
  - RULE-SET,private_domain,⭐ 国内
  - RULE-SET,apple_domain,🍎 Apple
  - RULE-SET,proxylite,🐾 国外
  - RULE-SET,ai,⚛ AI
  - RULE-SET,github_domain,🕊️ GitHub
  - RULE-SET,youtube_domain,❄️ YouTube
  - RULE-SET,google_domain,🕸️ Google
  - RULE-SET,tiktok_domain,👻 TikTok
  - RULE-SET,speedtest_domain,🐬 检测
  - RULE-SET,telegram_domain,🐧 Telegram
  - RULE-SET,netflix_domain,☃️ Netflix
  - RULE-SET,gfw_domain,🐾 国外
  - RULE-SET,geolocation-!cn,🐾 国外 # Changed to 🐾 国外 to align with "国外" group usage
  - RULE-SET,cn_domain,🎯 全球直连 # Changed to 🎯 全球直连 to align with "⭐ 国内" group
  - RULE-SET,google_ip,🍀 Google,no-resolve # Kept, assuming IP rules are still desired
  - RULE-SET,netflix_ip,🎥 NETFLIX,no-resolve # Kept, assuming IP rules are still desired
  - RULE-SET,telegram_ip,📲 Telegram,no-resolve # Kept, assuming IP rules are still desired
  - RULE-SET,cn_ip,🎯 全球直连 # Changed to 🎯 全球直连 to align with "⭐ 国内" group
  - MATCH,🐟 漏网之鱼
# 规则集
rule-anchor:
  ip: &ip {type: http, interval: 86400, behavior: ipcidr, format: mrs}
  domain: &domain {type: http, interval: 86400, behavior: domain, format: mrs}
  class: &class {type: http, interval: 86400, behavior: classical, format: text}
rule-providers:
  "⚛ AI": { <<: *class, url: "https://raw.githubusercontent.com/mxcz5359/bookish-engine/refs/heads/main/ai.list"}
